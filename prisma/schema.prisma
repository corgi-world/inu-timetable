generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model User {
  id       String @id
  nickname String @unique
  password String
}

model Timetables {
  index       Int    @id @default(autoincrement())
  id          String
  nickname    String
  semester    String
  college     String
  major       String
  grade       String
  totalGrades Int
  timetables  Json
}
